package object_fecha;

import java.text.ParseException;

public class CRUD {
	public static Fecha create() throws ParseException  {
		
		Object fecha = null;
		
		int dia = validadors.validar_int("Dime un dia", "dia");
		int mes = validadors.validar_int("Dime un mes", "mes");
		int ano = validadors.validar_int("Dime un ano", "ano");
		
		fecha = new Fecha(dia, mes, ano);
		return fecha;
	}
}
