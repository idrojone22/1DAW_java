package tema4_v13.modules.users.utils;

import java.util.Calendar;

import tema4_v13.classes.Fecha;

public class func_sueldo {
	public int calcularSueldo(Fecha fecha_alta) {
		int sueldoBase = 1400;
		int incremento = 25;
		
		  Calendar fechaActual = Calendar.getInstance();
		  
		  int anioAlta = fecha_alta.getAnio(); // Suponiendo que Fecha tiene un método getAnio()
		  int mesAlta = this.fecha_alta.getMes(); // Suponiendo que Fecha tiene un método getMes()
		  int diaAlta = this.fecha_alta.getDia();
	}
}
