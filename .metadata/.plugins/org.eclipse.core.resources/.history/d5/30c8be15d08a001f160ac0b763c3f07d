package Framework;

import javax.swing.JOptionPane;

public class doble_bucle {
	
	public static void digitos(int num)  {
		
		int copia = 0;
		int digito = 0;
		String cad_user = "";
		
		/*IMPORTANT*/
		copia = num;
		/*IMPORTANT*/
		
		/*SEGUNDO BUCLE	*/
		while (num > 0) {
			digito = num % 10;
			num = num / 10;
			cad_user = cad_user
			JOptionPane.showMessageDialog(null, "Los digitos de " + copia + " son " + digito);
		}
		/*SEGUNDO BUCLE	*/
	}
}
