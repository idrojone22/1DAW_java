	public static void main(String[] args) {
		Object[] op_ID = convertirArray(array_desarrollo);

	}
		public static Object[] convertirArray(ArrayList<Desarrollo_web> arrayDesarrollo) {
		    Object[] op_ID = new Object[arrayDesarrollo.size()];
		  
		    for (int i = 0; i < arrayDesarrollo.size(); i++) {
		        op_ID[i] = arrayDesarrollo.get(i).toString(); // AsegÃºrate de que `toString()` devuelva lo que necesitas.
		    }
		    return op_ID;	
		    
		}

}
